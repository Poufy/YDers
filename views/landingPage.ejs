<!DOCTYPE html>
<html lang="en">
  <head>
    <!--Import Google Icon Font-->
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <!--Import materialize.css-->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />

    <link rel="stylesheet" href="style.css" />

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <body>
    <div class="scrollspy" id="home">
      <!-- HEADER -->
      <div class="navbar-fixed">
        <nav>
          <div class="nav-wrapper white">
            <div class="container">
              <img
                src="img/logo.jpg"
                style="height: 55px; width: 65px; padding-top: 5px"
              />
              <a href="#!" class="brand-logo black-text">
                <span class="secondaryTextColor">Tie</span>Co
              </a>

              <a href="#" data-target="mobile-nav" class="sidenav-trigger">
                <i class="material-icons black-text">menu</i>
              </a>
              <ul class="right hide-on-med-and-down">
                <li>
                  <a href="#home" class="secondaryTextColor">
                    المقدمة
                  </a>
                </li>
                <li>
                  <a href="#teachers" class="black-text">
                    أساتذتنا
                  </a>
                </li>
                <li>
                  <a href="#about" class="black-text">
                    من نحن؟
                  </a>
                </li>
                <li>
                  <a href="#contact" class="black-text">
                    التواصل
                  </a>
                </li>
                <li class="secondaryColor">
                  <a href="#register" class="white-text">
                    التسجيل
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </nav>
      </div>
      <ul class="sidenav" id="mobile-nav">
        <li>
          <a href="#home" class="secondaryTextColor">
            المقدمة
          </a>
        </li>
        <li>
          <a href="#teachers" class="black-text">
            أساتذتنا
          </a>
        </li>
        <li>
          <a href="#about" class="black-text">
            من نحن؟
          </a>
        </li>
        <li>
          <a href="#contact" class="black-text">
            التواصل
          </a>
        </li>
        <li>
          <a href="#register" class="black-text">
            التسجيل
          </a>
        </li>
      </ul>
      <!-- END OF HEADER -->
      <!-- START OF SLIDER-->
      <div class="slider fullscreen seperator">
        <ul class="slides">
          <li>
            <img class="responsive-img" src="img/slider2.jpg" alt="lmao" />
            <div class="caption center-align">
              <h3 class="primaryTextColor">شعار يقشعر البدن</h3>
              <h5 class="light primaryTextColor hide-on-small-only">
                الخدمات التي نوفرها
              </h5>
              <a
                href="#contact"
                class="btn light-blue accent-3"
                style="margin-top: 20px"
              >
                تواصل معنا
              </a>
            </div>
          </li>
          <li>
            <img src="img/slider1.jpg" alt="lmao" />
            <div class="caption right-align primaryTextColor">
              <h3 class="primaryTextColor">أهدافنا</h3>
              <h5 class="light primaryTextColor hide-on-small-only">
                وحدة حرية اشتراكية
              </h5>
              <a
                href="#contact"
                class="btn light-blue accent-3"
                style=" margin-top: 20px"
              >
                قم بالتسجيل
              </a>
            </div>
          </li>
          <li>
            <img class="img3" src="img/slider3.jpg" alt="lmao" />
            <div class="caption left-align primaryTextColor">
              <h3 class="primaryTextColor">صفاتنا</h3>
              <h5 class="light primaryTextColor hide-on-small-only">
                ما شاء الله علينا كلنا حبابين ونسعى للأفضل
              </h5>
              <a
                href="#about"
                class="btn light-blue accent-3"
                style=" margin-top: 20px"
              >
                المزيد
              </a>
            </div>
          </li>
        </ul>
      </div>
      <!-- END OF SLIDER-->
      <!--CARD PANELS OF AVAIABLE TEACHERS-->
      <div class="content">
      <section
        class="section section-follow light-blue darken2 white-text center seperator scrollspy"
        id="teachers"
      >
      <h4 class="center styledHeader" style="padding-bottom: 18px">
أساتذتنا          </h4>
      <div class="container">
            <div class="row">
                <div class="col s6 m3">
                    <div class="card-panel light-blue lighten-1">
                        <img src="img/mathIcon2.png" alt="Math" style="height: 60px;width:60px"><br>
                        <h5>الرياضيات</h5>
                        <div class="mathTeachers" style="font-weight: 500;">
                            <h5>6</h5>
                        </div>
                    </div>
                </div>
                <div class="col s6 m3">
                        <div class="card-panel light-blue lighten-1">
                            <img src="img/chemIcon.png" alt="Math" style="height: 60px;width:60px"><br>
                            <h5>الكيمياء</h5>
                            <div class="mathTeachers" style="font-weight: 500;">
                                <h5>4</h5>
                            </div>
                        </div>
                    </div>
                    <div class="col s6 m3">
                            <div class="card-panel light-blue lighten-1">
                                <img src="img/physIcon2.png" alt="Math" style="height: 60px;width:60px"><br>
                                <h5>الفيزياء</h5>
                                <div class="mathTeachers" style="font-weight: 500;">
                                    <h5>7</h5>
                                </div>
                            </div>
                        </div>
                        <div class="col s6 m3">
                                <div class="card-panel light-blue lighten-1">
                                    <img src="img/bioIcon2.png" alt="Math" style="height: 60px;width:60px"><br>
                                    <h5>العلوم</h5>
                                    <div class="mathTeachers" style="font-weight: 500;">
                                        <h5>6</h5>
                                    </div>
                                </div>
                            </div>
            </div>
            <div class="row">
                    <div class="col s6 m3">
                        <div class="card-panel light-blue lighten-1">
                            <img src="img/arIcon.png" alt="Math" style="height: 60px;width:60px"><br>
                            <h5>العربية</h5>
                            <div class="mathTeachers" style="font-weight: 500;">
                                <h5>6</h5>
                            </div>
                        </div>
                    </div>
                    <div class="col s6 m3">
                            <div class="card-panel light-blue lighten-1">
                                <img src="img/engIcon2.png" alt="Math" style="height: 60px;width:60px"><br>
                                <h5>الإنجليزية</h5>
                                <div class="mathTeachers" style="font-weight: 500;">
                                    <h5>14</h5>
                                </div>
                            </div>
                        </div>
                        <div class="col s6 m3">
                                <div class="card-panel light-blue lighten-1">
                                    <img src="img/frenIcon.png" alt="Math" style="height: 60px;width:60px"><br>
                                    <h5>الفرنسية</h5>
                                    <div class="mathTeachers" style="font-weight: 500;">
                                        <h5>5</h5>
                                    </div>
                                </div>
                            </div>
                            <div class="col s6 m3">
                                    <div class="card-panel light-blue lighten-1">
                                        <img src="img/geoIcon.png" alt="Math" style="height: 60px;width:60px"><br>
                                        <h5>الجغرافية</h5>
                                        <div class="mathTeachers" style="font-weight: 500;">
                                            <h5>3</h5>
                                        </div>
                                    </div>
                                </div>
                </div>
          </div>
        </section>
        <!--START OF FORM-->
        <section class="section section-follow light-blue darken2 white-text center seperator scrollspy" id="apply">
<h4>التقديم</h4>
        </section>
      </div>
    </div>
    </div>
  </body>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script>
    document.addEventListener("DOMContentLoaded", function() {
      var elems = document.querySelectorAll(".sidenav");
      var instances = M.Sidenav.init(elems, { draggable: true });
    });
  </script>
  <script>
    var elems = document.querySelectorAll(".slider");
    var instances = M.Slider.init(elems, {
      indicators: true
    });
  </script>
  <script>document.addEventListener('DOMContentLoaded', function() {
        var elems = document.querySelectorAll('.scrollspy');
        var instances = M.ScrollSpy.init(elems, {});
      });</script>
</html>
