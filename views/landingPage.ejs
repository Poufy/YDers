<!DOCTYPE html>
<html lang="en">
  <head>
    <!--Import Google Icon Font-->
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <!--Import materialize.css-->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />

    <link rel="stylesheet" href="style.css" />

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <body>
    <div class="scrollspy" id="home">
      <!-- HEADER -->
      <div class="navbar-fixed">
        <nav>
          <div class="nav-wrapper white">
            <div class="container">
              <img
                src="img/logo.jpg"
                style="height: 55px; width: 65px; padding-top: 5px"
              />
              <a href="#!" class="brand-logo black-text">
                <span class="secondaryTextColor">Tie</span>Co
              </a>

              <a href="#" data-target="mobile-nav" class="sidenav-trigger">
                <i class="material-icons black-text">menu</i>
              </a>
              <ul class="right hide-on-med-and-down">
                <li>
                  <a href="#home" class="secondaryTextColor">
                    Home
                  </a>
                </li>
                <li>
                  <a href="#about" class="black-text">
                    About Us
                  </a>
                </li>
                <li>
                  <a href="#contact" class="black-text">
                    Contact
                  </a>
                </li>
                <li>
                  <a href="#register" class="black-text">
                    Register
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </nav>
      </div>
      <ul class="sidenav" id="mobile-nav">
        <li>
          <a href="#home" class="black-text">
            Home
          </a>
        </li>
        <li>
          <a href="#about" class="black-text">
            About Us
          </a>
        </li>
        <li>
          <a href="#contact" class="black-text">
            Contact
          </a>
        </li>
        <li>
          <a href="#register" class="black-text">
            Register
          </a>
        </li>
      </ul>
      <!-- END OF HEADER -->
      <!-- START OF SLIDER-->
      <div class="slider fullscreen seperator">
        <ul class="slides">
          <li>
            <img class="responsive-img" src="img/slider2.jpg" alt="lmao" />
            <div class="caption center-align">
              <h3 class="primaryTextColor">شعار يقشعر البدن</h3>
              <h5 class="light primaryTextColor hide-on-small-only">
                الخدمات التي نوفرها
              </h5>
              <a
                href="#contact"
                class="btn light-blue accent-3"
                style="margin-top: 20px"
              >
                تواصل معنا
              </a>
            </div>
          </li>
          <li>
            <img src="img/slider1.jpg" alt="lmao" />
            <div class="caption right-align primaryTextColor">
              <h3 class="primaryTextColor">أهدافنا</h3>
              <h5 class="light primaryTextColor hide-on-small-only">
                وحدة حرية اشتراكية
              </h5>
              <a
                href="#contact"
                class="btn light-blue accent-3"
                style=" margin-top: 20px"
              >
                قم بالتسجيل
              </a>
            </div>
          </li>
          <li>
            <img class="img3" src="img/slider3.jpg" alt="lmao" />
            <div class="caption left-align primaryTextColor">
              <h3 class="primaryTextColor">صفاتنا</h3>
              <h5 class="light primaryTextColor hide-on-small-only">
                ما شاء الله علينا كلنا حبابين ونسعى للأفضل
              </h5>
              <a
                href="#about"
                class="btn light-blue accent-3"
                style=" margin-top: 20px"
              >
                المزيد
              </a>
            </div>
          </li>
        </ul>
      </div>
      <!-- END OF SLIDER-->
    </div>
  </body>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script>
    document.addEventListener("DOMContentLoaded", function() {
      var elems = document.querySelectorAll(".sidenav");
      var instances = M.Sidenav.init(elems, { draggable: true });
    });
  </script>
  <script>
    var elems = document.querySelectorAll(".slider");
    var instances = M.Slider.init(elems, {
      indicators: true
    });
  </script>
</html>
